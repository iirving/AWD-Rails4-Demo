<h1>Store#index More 222222t</h1>

<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<% @products.each do |product| %>
	<div class="entry">
		<%= image_tag(product.image_url) %>
		<h3><%= product.title %> </h3>
		<p><%= sanitize(product.description) %></p>
		<div class="price_line">
			<span class="price">$<%= product.price %></span>
		</div>
	</div>
<% end %>